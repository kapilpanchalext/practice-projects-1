package enumdemo;

public class EnumDemo {

	enum Operation{
		PLUS {
	        double eval(double x, double y) { return x + y; }
	    },
	    MINUS {
	        double eval(double x, double y) { return x - y; }
	    },
	    TIMES {
	        double eval(double x, double y) { return x * y; }
	    },
	    DIVIDED_BY {
	        double eval(double x, double y) { return x / y; }
	    }
	};
		
	// Each constant supports an arithmetic operation
   
    
	public static void main(String[] args) {
		 
		
	}
}